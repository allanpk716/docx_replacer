{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "FileProcessing": {
    "MaxRetryAttempts": 3,
    "RetryDelayMilliseconds": 500,
    "EnableDetailedLogging": true
  },
  "Performance": {
    "EnableTemplateCaching": true,
    "MaxCacheSize": 10,
    "CacheExpirationMinutes": 30
  }
}
